{
    "npmScope": "nx-mono",
    "affected": {
        "defaultBase": "master"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": [
                    "build",
                    "lint",
                    "test",
                    "e2e",
                    "build-storybook"
                ]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "projects": {
        "demoapp": {
            "tags": []
        },
        "demoapp-e2e": {
            "tags": [],
            "implicitDependencies": ["demoapp"]
        },
        "mycomps": {
            "tags": []
        }
    }
}
